  "version": "0.0.0",
  "type": "module",
  "homepage": "https://mauroalderete.dev",
  "scripts": {
    "dev": "vite",
    "build:dev": "npm run lint && tsc && vite build",
    "build:prod": "npm run lint && npm run test && npm run test:e2e && npm run coverage && tsc && vite build",
